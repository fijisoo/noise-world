{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": false,
      "targetField": "title"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "keywords": {
      "type": "string"
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "Markdown",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
